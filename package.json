{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "subset": "tsx scripts/font-subset.ts",
    "prebuild": "npm run index:generate && npm run subset && npm run format",
    "predev": "npm run index:generate",
    "dev": "vite",
    "build": "npm run check && vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "lint:markup": "tsx scripts/markuplint-changed.ts",
    "lint:markup:all": "markuplint \"src/**/*.vue\"",
    "check": "npm-run-all --parallel lint lint:markup prettier:check",
    "prettier:check": "prettier --check .",
    "index:generate": "tsx scripts/generate-article-index.ts",
    "fix": "npm-run-all --parallel lint:fix format",
    "title": "npx tsx scripts/check-title-mismatch.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-query": "^5.92.5",
    "dompurify": "^3.3.1",
    "swiper": "^12.0.3",
    "tailwindcss": "^4.0.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@markuplint/vue-parser": "^4.6.22",
    "@markuplint/vue-spec": "^4.5.22",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "lodash": "^4.17.21",
    "markuplint": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
